{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "esycat/upsource",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/data",
      "ContainerDirectory": "/var/lib/upsource/data/"
    },
    {
      "HostDirectory": "/var/app/backups",
      "ContainerDirectory": "/var/lib/upsource/backups/"
    }
  ],
  "Logging": "/var/lib/upsource/log/"
}

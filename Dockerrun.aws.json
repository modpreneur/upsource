{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "jetbrains/upsource:2017.1.1922",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/data",
      "ContainerDirectory": "/opt/upsource/data"
    },
    {
      "HostDirectory": "/var/app/conf",
      "ContainerDirectory": "/opt/upsource/conf"
    },
    {
      "HostDirectory": "/var/app/backups",
      "ContainerDirectory": "/opt/upsource/backups"
    }
  ],
  "Logging": "/var/lib/upsource/log/"
}

{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "esycat/upsource",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/data",
      "ContainerDirectory": "/var/lib/upsource"
    },
    {
      "HostDirectory": "/var/app/conf",
      "ContainerDirectory": "/opt/upsource/conf"
    }
  ],
  "Logging": "/var/lib/upsource/log/"
}
